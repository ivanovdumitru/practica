{% extends 'B2BBlogBundle:template:base.html.twig' %}
{% block content %}
<div class="row">
    {{ dump(data) }}
    {% if false %}
    {% if data|length  %}
    <div class="col-sm-4">
        {% if data.image %}
        <img src="{{ data.image }}" alt="{{ data.name }}" class="profile-logo" />
    </div>
    <div class="col-sm-8">
        <ul class="list-unstyled">
            <li><h1>{{ data.name }}</h1><br></li>
            <li>
                <p>about our company:</p>
                {{ data.description }}
                <br>
            </li>
            <li>
                <p>phones:</p>
                <ul class="list-unstyled">
                    {% for phone in data.phones %}
                        <li>{{ phone }}</li>
                    {% endfor %}
                </ul>
                <br>
            </li>
            <li>
                <p>emails:</p>
                <ul class="list-unstyled">
                    {% for email in data.emails %}
                        <li>{{ email }}</li>
                    {% endfor %}
                </ul>
            </li>
        </ul>
    </div>
<hr>
    <div class="text-center"><a href="#" class="btn btn-default disabled">Visit store</a></div>
    {% endif %}
{% endif %}
</div>
{% endblock %}
